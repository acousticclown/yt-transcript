{
  "buildCommand": "cd ../.. && npm install && cd apps/web && npm run build",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "@api_url"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "@api_url/api/:path*"
    }
  ]
}

